set list {list} Table.pas
set list {list} Symbol.pas Symbol.Print
set list {list} Scanner.pas Scanner.debug
set list {list} Asm.pas
set list {list} Expression.pas
set list {list} Parser.pas
set list {list} CGC.pas
set list {list} CGI.pas CGI.Comments CGI.Debug
set list {list} ObjOut.pas
set list {list} Native.pas
set list {list} DAG.pas
set list {list} Gen.pas
set list {list} Header.pas

echo -n "Pascal: "
wc -l {list}

